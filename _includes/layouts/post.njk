---
layout: layouts/base.njk
templateClass: tmpl-post
---
  <div class="head">
    <h2>{{ title }}</h2>
    <h3>{{ type }}</h3>
  </div>
  
  <div class="content">
    <div class="sidebar">
      <img src="{{ img }}">
    </div>
    <div class="article">
      {{ content | safe }}
    </div>
  </div>

